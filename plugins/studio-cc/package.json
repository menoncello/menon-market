  "devDependencies": {
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "bun-types": "latest"
  },
  "engines": {
    "bun": ">=1.0.0"
