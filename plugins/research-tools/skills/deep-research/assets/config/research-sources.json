  "quality_thresholds": {
    "minimum_sources_per_claim": 3,
    "maximum_source_age_months": {
      "technology": 12,
      "business": 18,
      "financial": 6,
      "market": 12,
      "general": 24
    },
    "authority_weights": {
      "academic": 0.3,
      "government": 0.25,
      "industry": 0.2,
      "news": 0.15,
      "other": 0.1
    },
    "recency_weights": {
      "last_3_months": 0.4,
      "last_6_months": 0.3,
      "last_12_months": 0.2,
      "older": 0.1
    }
  },
